{
  "name": "vue-landing-pages",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:prod": "run-p \"build-only --mode production\" --",
    "build:test": "run-p \"build-only --mode test\" --",
    "build:dev": "run-p \"build-only --mode development\" --",
    "preview": "vite preview",
    "build-only": "vite build --mode production",
    "type-check": "vue-tsc --build --force",
    "lint": "npx oxlint. --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@vueuse/core": "^12.8.2",
    "axios": "^1.9.0",
    "element-plus": "^2.9.11",
    "es-toolkit": "^1.38.0",
    "gsap": "^3.13.0",
    "ogl": "^1.0.11",
    "pinia": "^2.3.1",
    "vue": "^3.5.15",
    "vue-i18n": "^10.0.7",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@inspira-ui/plugins": "^0.0.1",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@playwright/test": "^1.52.0",
    "@tsconfig/node20": "^20.1.5",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.23",
    "@unocss/preset-rem-to-px": "^0.65.4",
    "@unocss/transformer-directives": "^0.65.4",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.16.12",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "sass": "^1.89.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "unocss": "^0.65.4",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.28.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-oxlint": "^1.3.1",
    "vite-plugin-pages": "^0.32.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.2.10"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,astro,svelte}": "oxlint"
  },
  "types": "./types/index.d.ts",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ],
    "overrides": {
      "vite": "npm:rolldown-vite@latest"
    }
  }
}